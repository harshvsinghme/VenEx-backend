{
  "name": "venex-backend",
  "version": "1.0.0",
  "description": "venex-backend",
  "main": "index.ts",
  "scripts": {
    "local": "NODE_ENV=local ts-node-dev index.ts",
    "local:win": "set NODE_ENV=local&&ts-node-dev index.ts",
    "dev": "NODE_ENV=development node dist/index.js",
    "format": "prettier --write './**/*.{ts,md,json}' --config ./.prettierrc",
    "build": "tsc --project tsconfig.json",
    "lint": "rimraf dist && eslint . --fix",
    "prisma:dev": "DATABASE_URL=mysql://root:my-secret-pw@localhost:3306/venex prisma migrate dev",
    "prisma:dev:win": "set DATABASE_URL=mysql://root:my-secret-pw@localhost:3306/venex&&prisma migrate dev",
    "prisma:deploy": "DATABASE_URL=mysql://root:my-secret-pw@localhost:3306/venex prisma migrate deploy",
    "prisma:deploy:win": "set DATABASE_URL=mysql://root:my-secret-pw@localhost:3306/venex&&prisma migrate deploy",
    "prisma:reset": "DATABASE_URL=mysql://root:my-secret-pw@localhost:3306/venex prisma migrate reset -f",
    "prisma:reset:win": "set DATABASE_URL=mysql://root:my-secret-pw@localhost:3306/venex&&prisma migrate reset -f",
    "prisma:generate": "prisma generate",
    "prisma:format": "prisma format",
    "prisma:studio": "DATABASE_URL=mysql://root:my-secret-pw@localhost:3306/venex prisma studio",
    "prisma:studio:win": "set DATABASE_URL=mysql://root:my-secret-pw@localhost:3306/venex&&prisma studio"
  },
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "^9.12.0",
    "@types/cors": "^2.8.17",
    "@types/eslint__js": "^8.42.3",
    "@types/express": "^5.0.0",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.7.5",
    "eslint": "^9.12.0",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.9.0"
  },
  "dependencies": {
    "@prisma/client": "^6.0.1",
    "cors": "^2.8.5",
    "dayjs": "^1.11.13",
    "express": "^4.21.1",
    "http-status-codes": "^2.3.0",
    "morgan": "^1.10.0",
    "prettier-plugin-organize-imports": "^4.1.0",
    "prisma": "^6.0.1",
    "winston": "^3.15.0",
    "zod": "^3.23.8",
    "zod-validation-error": "^3.4.0"
  }
}
